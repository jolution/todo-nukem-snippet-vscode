{
  "name": "todo-nukem-generator-vscode",
  "displayName": "TODO NUKEM Generator",
  "author": "jolution",
  "publisher": "jolution",
  "repository": "https://github.com/jolution/todo-nukem-snippet-vscode",
  "bugs": "https://github.com/jolution/todo-nukem/issues",
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#e8bf28",
    "theme": "dark"
  },
  "description": "Generator for TODO NUKEM Convention",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.88.0"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "categories": [
    "Snippets",
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "tags": [
    "typescript",
    "javascript",
    "snippet",
    "documentation"
  ],
  "scripts": {
    "prepublish": "npm run compile",
    "package": "npx vsce package",
    "publish": "npx vsce publish",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "open-in-browser": "vscode-test-web --extensionDevelopmentPath=. .",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "contributes": {
    "commands": [
      {
        "command": "todo-nukem-inserter.insertTodo",
        "title": "Insert TODO NUKEM Comment"
      }
    ],
    "snippets": [
      {
        "language": "typescript",
        "path": "./snippets/snippets-line-comment.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/snippets-line-comment.json"
      },
      {
        "language": "css",
        "path": "./snippets/snippets-block-comment.json"
      },
      {
        "language": "postcss",
        "path": "./snippets/snippets-block-comment.json"
      },
      {
        "language": "scss",
        "path": "./snippets/snippets-block-comment.json"
      },
      {
        "language": "less",
        "path": "./snippets/snippets-block-comment.json"
      },
      {
        "language": "html",
        "path": "./snippets/snippets-block-comment.json"
      },
      {
        "language": "python",
        "path": "./snippets/snippets-block-comment.json"
      },
      {
        "language": "java",
        "path": "./snippets/snippets-block-comment.json"
      },
      {
        "language": "csharp",
        "path": "./snippets/snippets-block-comment.json"
      },
      {
        "language": "cpp",
        "path": "./snippets/snippets-block-comment.json"
      },
      {
        "language": "ruby",
        "path": "./snippets/snippets-block-comment.json"
      },
      {
        "language": "swift",
        "path": "./snippets/snippets-block-comment.json"
      },
      {
        "language": "php",
        "path": "./snippets/snippets-block-comment.json"
      },
      {
        "language": "go",
        "path": "./snippets/snippets-block-comment.json"
      },
      {
        "language": "rust",
        "path": "./snippets/snippets-block-comment.json"
      },
      {
        "language": "dart",
        "path": "./snippets/snippets-block-comment.json"
      },
      {
        "language": "perl",
        "path": "./snippets/snippets-block-comment.json"
      },
      {
        "language": "lua",
        "path": "./snippets/snippets-block-comment.json"
      },
      {
        "language": "shellscript",
        "path": "./snippets/snippets-block-comment.json"
      }
    ]
  },
  "dependencies": {
    "@vscode/vsce": "^3.7.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/vscode": "^1.88.0",
    "@vscode/test-web": "^0.0.75",
    "typescript": "^5.9.3"
  }
}
